package com.ipnotica.menu.header {		import flash.display.MovieClip;	import flash.events.MouseEvent;	public class Upload extends MovieClip {				public function Upload() {			super();			init();		}				private function init():void {			this.buttonMode = true;			initEvents();		}				private function initEvents():void {			addEventListener(MouseEvent.CLICK, onClickUpload);		}				private function onClickUpload(e:MouseEvent):void {			trace("Now you can upload new stuff");		}			}}